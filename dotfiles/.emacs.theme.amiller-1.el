(set-background-color "white")
(set-foreground-color "black")
(set-cursor-color     "red")
(set-face-background vvb-face "light goldenrod yellow")

(custom-set-faces
 '(jde-java-font-lock-code-face     ((t (                             :bold nil))))
 '(font-lock-comment-face           ((t (:foreground "DarkGreen"      :bold nil :italic t))))
 '(jde-java-font-lock-link-face     ((t (:foreground "SkyBlue4"       :bold nil :italic t :underline t))))
 '(jde-java-font-lock-doc-tag-face  ((t (:foreground "skyblue4"       :bold t   :italic t))))
 '(jde-java-font-lock-pre-face      ((t (:foreground "DarkSlateGray4" :bold nil :italic nil))))

 '(font-lock-builtin-face           ((t (:foreground "DarkOrchid"     :bold nil :italic nil))))
 '(font-lock-keyword-face           ((t (:foreground "SteelBlue4"     :bold nil :italic nil))))
 '(jde-java-font-lock-modifier-face ((t (:foreground "Orchid4"        :bold nil :italic nil))))
 '(jde-java-font-lock-package-face  ((t (:foreground "blue"           :bold nil :italic nil))))

 '(font-lock-type-face              ((t (:foreground "blue3"          :bold nil :italic nil))))
 '(font-lock-function-name-face     ((t (:foreground "maroon4"        :bold nil))))
 '(font-lock-variable-name-face     ((t (:foreground "magenta4"       :bold nil :italic nil))))
 '(font-lock-reference-face         ((t (:foreground "red"))))

 '(font-lock-constant-face          ((t (:foreground "red"            :bold nil :italic nil))))
 '(font-lock-string-face            ((t (:foreground "brown"          :bold nil :italic nil))))
 '(jde-java-font-lock-number-face   ((t (:foreground "red"            :bold nil :italic nil))))
 )
